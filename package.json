{
  "name": "server-side-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^9.1.2",
    "express": "^4.21.2",
    "ignore-styles": "^5.0.1",
    "nodemon": "^3.1.10",
    "react": "^19.0.0",
    "react-dom": "^19.1.1",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.7.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "frontend": "react-scripts start",
    "frontend:build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "backend": "NODE_ENV=production node index.cjs",
    "app:start": "concurrently \"npm run frontend\" \"npm run backend\"",
    "start": "NODE_ENV=production node server.js",
    "build": "npm install && npm run frontend:build",
    "format": "prettier . --write",
    "config": "node --env-file=.env --experimental-config-file=app.config.json index.cjs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.0.4",
    "jest": "^27.5.1",
    "prettier": "^3.6.2"
  }
}
